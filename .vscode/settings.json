{
    "files.exclude": {
        "node_modules": true,
        "bower_components": true,
        "build/js": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/bin": true,
        "**/obj": true,
        "dist": true,
        ".vs": true,
        "*.sln": true,
        "*.csproj*": true,
        "web*.config": true,
    },
    "search.exclude": {
        "dist": true,
        "resources/js": true
    },
    "files.encoding": "utf8",
    "files.eol": "\r\n",
    "editor.codeActionsOnSave": {
        "source.fixAll.tslint": true
    },
    "editor.tabSize": 4,
    "typescript.tsdk": "node_modules/typescript/lib/",
    "cSpell.words": [
        "DTOs",
        "Geolocate",
        "Rerender",
        "Sublayer",
        "activedescendant",
        "argb",
        "autocast",
